{
  "name": "@sandstack/neuron",
  "description": "Neuron is a lightweight framework agnostic global state manager for Javascript apps, with React support",
  "version": "0.0.0",
  "author": "David A. Sanders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjrdave/Neuron/issues"
  },
  "homepage": "https://sandstack.dev/neuron",
  "type": "module",
  "sideEffects": false,
  "main": "./index.js",
  "types": "./vanilla/index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "files": [
    "**"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./CONTRIBUTING.md": "./CONTRIBUTING.md",
    ".": {
      "require": {
        "types": "./vanilla/index.d.ts",
        "default": "./index.cjs"
      },
      "import": {
        "types": "./vanilla/index.d.ts",
        "default": "./index.js"
      },
      "module": {
        "types": "./vanilla/index.d.ts",
        "default": "./index.js"
      },
      "default": {
        "types": "./vanilla/index.d.ts",
        "default": "./index.js"
      }
    },
    "./react": {
      "require": {
        "types": "./react/index.d.ts",
        "default": "./react.cjs"
      },
      "import": {
        "types": "./react/index.d.ts",
        "default": "./react.js"
      },
      "module": {
        "types": "./react/index.d.ts",
        "default": "./react.js"
      },
      "default": {
        "types": "./react/index.d.ts",
        "default": "./react.js"
      }
    },
    "./slices": {
      "require": {
        "types": "./slices/index.d.ts",
        "default": "./slices.cjs"
      },
      "import": {
        "types": "./slices/index.d.ts",
        "default": "./slices.js"
      },
      "module": {
        "types": "./slices/index.d.ts",
        "default": "./slices.js"
      },
      "default": {
        "types": "./slices/index.d.ts",
        "default": "./slices.js"
      }
    },
    "./persist": {
      "require": {
        "types": "./persist/index.d.ts",
        "default": "./persist.cjs"
      },
      "import": {
        "types": "./persist/index.d.ts",
        "default": "./persist.js"
      },
      "module": {
        "types": "./persist/index.d.ts",
        "default": "./persist.js"
      },
      "default": {
        "types": "./persist/index.d.ts",
        "default": "./persist.js"
      }
    }
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript",
    "typescript",
    "global-state-management",
    "state-manager",
    "global",
    "neuron",
    "neuronjs",
    "@sandstack",
    "@neuron",
    "sandstack",
    "vanilla-neuron"
  ]
}
