{
  "name": "@sandstack/neuron",
  "description": "Neuron is a lightweight framework agnostic global state manager for Javascript apps, with React support",
  "version": "0.0.0",
  "author": "David A. Sanders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjrdave/Neuron/issues"
  },
  "homepage": "https://sandstack.dev/neuron",
  "type": "module",
  "sideEffects": false,
  "main": "./vanilla/index.js",
  "types": "./index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "files": [
    "**"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./CONTRIBUTING.md": "./CONTRIBUTING.md",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./vanilla/index.cjs"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./vanilla/index.js"
      },
      "module": {
        "types": "./index.d.ts",
        "default": "./vanilla/index.js"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./vanilla/index.js"
      }
    },
    "./react": {
      "require": {
        "types": "./react.d.ts",
        "default": "./react/index.cjs"
      },
      "import": {
        "types": "./react.d.ts",
        "default": "./react/index.js"
      },
      "module": {
        "types": "./react.d.ts",
        "default": "./react/index.js"
      },
      "default": {
        "types": "./react.d.ts",
        "default": "./react/index.js"
      }
    },
    "./slices": {
      "require": {
        "types": "./slices.d.ts",
        "default": "./slices/index.cjs"
      },
      "import": {
        "types": "./slices.d.ts",
        "default": "./slices/index.js"
      },
      "module": {
        "types": "./slices.d.ts",
        "default": "./slices/index.js"
      },
      "default": {
        "types": "./slices.d.ts",
        "default": "./slices/index.js"
      }
    },
    "./persist": {
      "require": {
        "types": "./persist.d.ts",
        "default": "./persist/index.cjs"
      },
      "import": {
        "types": "./persist.d.ts",
        "default": "./persist/index.js"
      },
      "module": {
        "types": "./persist.d.ts",
        "default": "./persist/index.js"
      },
      "default": {
        "types": "./persist.d.ts",
        "default": "./persist/index.js"
      }
    }
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript",
    "typescript",
    "global-state-management",
    "state-manager",
    "global",
    "neuron",
    "neuronjs",
    "@sandstack",
    "@neuron",
    "sandstack",
    "vanilla-neuron"
  ]
}
